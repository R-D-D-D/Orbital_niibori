<% provide(:title, @title) %>
<table class="table table-dark">
  <thead>
  <tr>
    <th scope="col">Title</th>
    <th scope="col">Tutor</th>
  </tr>
  </thead>
  <tbody>
  <% @courses.each do |course| %>
    <tr>
      <td><%= link_to "#{course.title}", course_path(course) %></td>
      <td><%= link_to "#{course.tutor.name}", tutor_path(course.tutor) %></td>
    </tr>
  <% end %>
  </tbody>
</table>